{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/api",
  "targets": {
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "gen-contract": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx @openapitools/openapi-generator-cli generate -i libs/api/contract.yaml -g go -o libs/api/__generated__/contract"
      }
    }
  },
  "tags": []
}
